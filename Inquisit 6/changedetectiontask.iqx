<trial N4>
/ ontrialbegin = [
	values.N = 4;
	values.index = list.N4_indices.nextIndex;
	values.responseCategory = "";
	//sample square positions
	values.x1 = list.N4_positions_x1.nextValue;
	values.y1 = list.N4_positions_y1.nextValue;
