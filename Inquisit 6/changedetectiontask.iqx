	values.y3 = list.practiceN4_positions_y3.nextValue;
	values.x4 = list.practiceN4_positions_x4.nextValue;
	values.y4 = list.practiceN4_positions_y4.nextValue;
	//sample square colors
	values.color1 = list.practiceN4_colors_1.nextValue;
	values.color2 = list.practiceN4_colors_2.nextValue;
	values.color3 = list.practiceN4_colors_3.nextValue;
	values.color4 = list.practiceN4_colors_4.nextValue;
	//sample trialtype
	values.trialtype = list.practiceN4_trialTypes.nextValue;
	//sample probe position
	values.probe = list.practiceN4_probePosition.nextValue;
	if (values.probe == 1) {
		values.probe_x = values.x1;
		values.probe_y = values.y1;
	} else if (values.probe == 2) {
		values.probe_x = values.x2;
		values.probe_y = values.y2;
