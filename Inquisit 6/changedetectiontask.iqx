******************************************************************
Practice N = 4
******************************************************************

<trial practiceN4>
/ ontrialbegin = [
	values.N = 4;
	values.index = list.practiceN4_indices.nextIndex;
	values.responseCategory = "";
	//sample square positions
	values.x1 = list.practiceN4_positions_x1.nextValue;
	values.y1 = list.practiceN4_positions_y1.nextValue;
	values.x2 = list.practiceN4_positions_x2.nextValue;
	values.y2 = list.practiceN4_positions_y2.nextValue;
	values.x3 = list.practiceN4_positions_x3.nextValue;
	values.y3 = list.practiceN4_positions_y3.nextValue;
	values.x4 = list.practiceN4_positions_x4.nextValue;
	values.y4 = list.practiceN4_positions_y4.nextValue;
	values.x5 = "";
	values.y5 = "";
	values.x6 = "";
	values.y6 = "";
	values.x7 = "";
	values.y7 = "";
	values.x8 = "";
	values.y8 = "";
	//sample square colors
	values.color1 = list.practiceN4_colors_1.nextValue;
	values.color2 = list.practiceN4_colors_2.nextValue;
	values.color3 = list.practiceN4_colors_3.nextValue;
	values.color4 = list.practiceN4_colors_4.nextValue;
	values.color5 = "";
	values.color6 = "";
	values.color7 = "";
	values.color8 = "";
	//sample trialtype
	values.trialtype = list.practiceN4_trialTypes.nextValue;
	//sample probe position
	values.probe = list.practiceN4_probePosition.nextValue;
	if (values.probe == 1) {
		values.probe_x = values.x1;
		values.probe_y = values.y1;
	} else if (values.probe == 2) {
		values.probe_x = values.x2;
		values.probe_y = values.y2;
	} else if (values.probe == 3) {
		values.probe_x = values.x3;
		values.probe_y = values.y3;
	} else if (values.probe == 4) {
		values.probe_x = values.x4;
		values.probe_y = values.y4;
	}
	//sample probe color
	if (parameters.useOnlyArrayColorsForProbe) {
		values.probeColor = list.practiceN4_probeColor_current.nextValue;
	} else {
		values.probeColor = list.practiceN4_probeColor_all.nextValue;
	}
	trial.practiceN4.insertStimulusTime(shape.presentationSquare, parameters.arrayDuration);
	trial.practiceN4.insertStimulusTime(shape.probe, (parameters.arrayDuration + parameters.retentionInterval));
]
/ stimulustimes = [0 = clearScreen, canvas, presentationSquare, target1, target2, target3, target4, differentKeyReminder, sameKeyReminder]
/ validresponse = (parameters.leftResponsekey, parameters.rightResponsekey)
/ beginresponsetime = parameters.arrayDuration + parameters.retentionInterval
/ iscorrectresponse = [
	return((values.trialtype == 1 && trial.practiceN4.responseText == parameters.sameKey) ||
	(values.trialtype == 2 && trial.practiceN4.responseText == parameters.differentKey))	
]
/ ontrialend = [
	values.Feedback = trial.practiceN4.correct + 1;
	if (trial.practiceN4.responseText == parameters.sameKey) {
		values.responseCategory = "same";
	} else if (trial.practiceN4.responseText == parameters.differentKey) {
		values.responseCategory = "different";
	}
	//color Conversion for data file:
	values.color = values.probeColor;
	values.probeColor = expressions.colorConversion;
	values.color = values.color1;
	values.color1 = expressions.colorConversion;
	values.color = values.color2;
	values.color2 = expressions.colorConversion;
	values.color = values.color3;
	values.color3 = expressions.colorConversion;
	values.color = values.color4;
	values.color4 = expressions.colorConversion;
]

/ branch = [
	return trial.practiceN4_iti;
]
</trial>


<trial practiceN4_iti>
/ ontrialbegin = [
	text.sameKeyReminder_HL.skip = false;
	text.differentKeyReminder_HL.skip = false;
	if (trial.practiceN4.responseText == parameters.differentKey) {
		text.sameKeyReminder_HL.skip = true;
	} else {
		text.differentKeyReminder_HL.skip = true;
	}
]
/ stimulustimes = [0 = presentationSquare, sameKeyReminder_HL, differentKeyReminder_HL, Feedback; 50 = sameKeyReminder, differentKeyReminder]
/ validresponse = (parameters.leftResponsekey, parameters.rightResponsekey)//should show response buttons on touchscreen devices
/ isvalidresponse = [
	return false;
]
/ trialduration = parameters.FeedbackDuration
/ recorddata = false
</trial>

trial practiceN4Feedback>
/ ontrialbegin = [		
	values.probe_x += 0.5;
	values.probe_y += 0.5;
]
/ stimulustimes = [0 = presentationSquare, target1, target2, target3, target4, probe, Feedback]
/ timeout = parameters.FeedbackDuration
/ recorddata = false
/ posttrialpause = parameters.FeedbackDuration
/ responsemessage = (noresponse, clearScreen, 0)
/ responsemessage = (noresponse, shape.presentationSquare, 0)
/ responsemessage = (noresponse, text.differentKeyReminder, 0)
/ responsemessage = (noresponse, text.sameKeyReminder, 0)
</trial>

//Note: the following trial can be used to double check the placement of the squares/probe
<trial practiceN4_debug>
/ ontrialbegin = [
	values.N = 4;
