	if (values.probe == 1) {
		values.probe_x = values.x1;
		values.probe_y = values.y1;
	} else if (values.probe == 2) {
		values.probe_x = values.x2;
		values.probe_y = values.y2;
	} else if (values.probe == 3) {
		values.probe_x = values.x3;
		values.probe_y = values.y3;
	} else if (values.probe == 4) {
		values.probe_x = values.x4;
		values.probe_y = values.y4;
	} else if (values.probe == 5) {
		values.probe_x = values.x5;
		values.probe_y = values.y5;
	} else if (values.probe == 6) {
		values.probe_x = values.x6;
		values.probe_y = values.y6;
	} else if (values.probe == 7) {
		values.probe_x = values.x7;
		values.probe_y = values.y7;
	} else if (values.probe == 8) {
		values.probe_x = values.x8;
		values.probe_y = values.y8;
	}
	values.probe_x += 1.5%;
	values.probe_y += 1.5%;
	//sample probe color
	if (parameters.useOnlyArrayColorsForProbe) {
		values.probeColor = list.N8_probeColor_current.nextValue;
	} else {
		values.probeColor = list.N8_probeColor_all.nextValue;
	}
	//store all square distances presented:
	list.squareDistances.appendItem(expressions.d12);
	list.squareDistances.appendItem(expressions.d13);
